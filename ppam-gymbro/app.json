{
  "expo": {
    "name": "GymBRO",
    "slug": "ppam-gymbro",
    "version": "1.0.0",
    "scheme" : "gymbro",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#222222"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.azkara.ppamgymbro"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.azkara.ppamgymbro"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7bccb62e-2f8a-43e6-ad1a-52590f095c52"
      }
    },
    "plugins": [
      "expo-router"
      
    ]
  }
}
